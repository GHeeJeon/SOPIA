{
	"name": "sopia",
	"version": "1.0.0",
	"description": "A Spooncast manager A.I Program",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"build:win32": "build --win --ia32"
	},
	"build" : {
		"productName": "SOPIA",
		"appId": "com.electron.sopia",
		"asar": true,
		"protocols": {
			"name" : "sopia",
			"schemes" : ["helloSOPIA"]
		},
		"win": {
			"target": [
				"zip",
				"nsis"
			],
			"icon": "./icon.ico"
		},
		"nsis": {
			"oneClick": false,
			"allowToChangeInstallationDirectory": true
		},
		"directories": {
			"output": "dist/",
			"app": "."
		}
	},
	"repository": "https://github.com/mobbing/SOPIA",
	"keywords": [],
	"author": "mobbing",
	"license": "CC0-1.0",
	"devDependencies": {
		"electron": "^5.0.4"
	},
	"dependencies": {
		"axios": "^0.19.0",
		"websocket": "^1.0.29"
	}
}
